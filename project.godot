; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[Fmod]

General/auto_initialize=true
General/channel_count=1024
General/is_live_update_enabled=true
General/is_memory_tracking_enabled=false
"Software Format/sample_rate"=48000
"Software Format/speaker_mode"=3
"Software Format/raw_speaker_count"=0
General/default_listener_count=1
General/banks_path="res://fmod_banks/Desktop"
General/should_load_by_name=false
DSP/dsp_buffer_size=512
DSP/dsp_buffer_count=4
"3D Settings/doppler_scale"=1.0
"3D Settings/distance_factor"=1.0
"3D Settings/rolloff_scale"=1.0

[application]

config/name="ohye"
run/main_scene="res://test/item_test.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Shaker="*res://addons/shaker/src/Shaker.gd"
Global="*res://utils/global.gd"
FmodManager="*res://addons/fmod/FmodManager.gd"
Global2d="*res://2d/global/global_2d.gd"

[editor_plugins]

enabled=PackedStringArray("res://addons/fmod/plugin.cfg", "res://addons/func_godot/plugin.cfg", "res://addons/shaker/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/": "purple",
"res://components/": "orange",
"res://compositor_effects/": "gray",
"res://entities/": "orange",
"res://entities/player/": "orange",
"res://entities/player/sounds/": "blue",
"res://music/": "blue",
"res://shaders/": "gray",
"res://shakes/": "gray",
"res://utils/": "teal",
"res://world/": "orange"
}

